<div class="container">
  <h1>New Lesson</h1>
 
  <%= form_for @embedcard do |f| %>
    <%= render 'shared/errors', object: @embedcard %>
 
    <div class="form-group">
      <%= f.label :provider_url %>
      <%= f.text_field :provider_url, class: 'form-control', required: true %>
      <span class="help-block">Enter the link to a blog post.</span>
    </div>
 
    <%= f.submit class: 'btn btn-default' %>
  <% end %>
</div>
